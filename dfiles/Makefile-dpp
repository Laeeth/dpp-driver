run:
	d++ --compiler ldc2 --preprocess-only --no-sys-headers --ignore-specified-macros "min,max" --include-path ~/d-virtio/include/ --include-path ~/d-virtio/arch/x86/include/ dvirtio_net.dpp --clang-option "-nostdinc -isystem /usr/lib/gcc/x86_64-linux-gnu/8/include/ -I$(srctree)/arch/x86/include -I$(srctree)/arch/x86/include/generated  -I$(srctree)/include -I$(srctree)/arch/x86/include/uapi -I$(srctree)/arch/x86/include/generated/uapi -I$(srctree)/include/uapi -I$(srctree)/include/generated/uapi -include $(srctree)/include/linux/kconfig.h -D__KERNEL__ -Qunused-arguments -Wall -Wundef -Wstrict-prototypes -Wno-trigraphs -fno-strict-aliasing -fno-common -fshort-wchar -Werror-implicit-function-declaration -Wno-format-security -std=gnu89 -fno-PIE -mno-sse -mno-mmx -mno-sse2 -mno-3dnow -mno-avx -m64 -msoft-float -mregparm=3 -fno-pic -mstack-alignment=4 -ffreestanding -DCONFIG_AS_CFI=1 -DCONFIG_AS_CFI_SIGNAL_FRAME=1 -DCONFIG_AS_CFI_SECTIONS=1 -DCONFIG_AS_SSSE3=1 -DCONFIG_AS_CRC32=1 -DCONFIG_AS_AVX=1 -DCONFIG_AS_AVX2=1 -DCONFIG_AS_AVX512=1 -DCONFIG_AS_SHA1_NI=1 -DCONFIG_AS_SHA256_NI=1 -pipe -Wno-sign-compare -fno-asynchronous-unwind-tables -fno-delete-null-pointer-checks -O2 -UCC_HAVE_ASM_GOTO -Wframe-larger-than=1024 -fno-stack-protector -Wno-unused-variable -Wno-format-invalid-specifier -Wno-gnu -Wno-address-of-packed-member -Wno-tautological-compare -mno-global-merge -no-integrated-as -fno-omit-frame-pointer -fno-optimize-sibling-calls -g -gdwarf-4 -Wdeclaration-after-statement -Wno-pointer-sign -fno-strict-overflow -fno-stack-check -Werror=implicit-int -Werror=strict-prototypes -Werror=date-time -Werror=incompatible-pointer-types -Wno-initializer-overrides -Wno-unused-value -Wno-format -Wno-sign-compare -Wno-format-zero-length -Wno-uninitialized -DKBUILD_BASENAME='"dvirtio_net"' -DKBUILD_MODNAME='"dvirtio_net"'"
